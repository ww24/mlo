{
  "name": "mlo",
  "version": "1.0.0",
  "description": "Simple Module Loader",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover _mocha",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ww24/mlo.git"
  },
  "keywords": [
    "loader",
    "module"
  ],
  "author": "ww24",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ww24/mlo/issues"
  },
  "homepage": "https://github.com/ww24/mlo",
  "devDependencies": {
    "chai": "^2.2.0",
    "codeclimate-test-reporter": "0.0.4",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "dependencies": {
    "minimatch": "^2.0.5"
  }
}
